package com.sythiex.outerplanes;

import net.minecraft.world.WorldProvider;
import net.minecraft.world.biome.WorldChunkManager;
import net.minecraft.world.biome.WorldChunkManagerHell;
import net.minecraft.world.chunk.IChunkProvider;

public class WorldProviderElysium extends WorldProvider
{
	public void registerWorldChunkManager()
	{
		this.worldChunkMgr = new WorldChunkManagerHell(null, 1.0F);
		this.dimensionId = OuterPlanes.dimIDElysium;
	}
	
	public IChunkProvider createChunkGeneration()
	{
		return null;
	}
	
	@Override
	public String getDimensionName()
	{
		return "Elysium";
	}
}
